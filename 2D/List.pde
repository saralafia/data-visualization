class List {
  int month; 
  int year; 
  int java;
  int python;
  int php;
  int javascript;
  int perl;
  int ruby;

  List(int month, int year, int java, int python, int php, int javascript, int perl, int ruby)
  {
    this.month = month;
    this.year = year;
    this.java = java;
    this.python = python;
    this.php = php;
    this.javascript = javascript;
    this.perl = perl;
    this.ruby = ruby;
  }
}