class Book {
  int dewey;
  String title;
  String subject;
  int date; 
  int count;
  int days;
  int topic;
  String terms;
  
  Book(int dewey, String title, String subject, int date, int count, int days, int topic, String terms){
    this.dewey = dewey;
    this.title = title;
    this.subject = subject;
    this.date = date;
    this.count = count;
    this.days = days;
    this.topic = topic;
    this.terms = terms;
  }
}